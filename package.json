{
    "name": "vitals",
    "private": true,
    "scripts": {
        "lint": "oxlint --fix",
        "test": "bun test",
        "perf:client:dev": "bun run scripts/client-perf.ts --url http://localhost:3000 --runs 10 --threshold 250",
        "bloodwork:import": "bun run --env-file .env --env-file .env.local scripts/bloodwork-import.ts",
        "bloodwork:backfill": "bun run --env-file .env --env-file .env.local scripts/bloodwork-import.ts --all --continue-on-error --review-report-dir data/review",
        "data:sync": "bun run --env-file .env --env-file .env.local scripts/download-data.ts",
        "server:dev": "bun run --elide-lines 0 --no-clear-screen --install fallback --env-file .env --env-file .env.local --filter server dev",
        "client:dev": "bun run --elide-lines 0 --no-clear-screen --install fallback --env-file .env --env-file .env.local --filter client dev",
        "dev": "concurrently --restart-tries=-1 --restart-after=1000 --names 'server ,client ,' --c 'green,cyan' 'bun run server:dev' 'bun run client:dev'"
    },
    "dependencies": {
        "@ant-design/icons": "^6.1.0",
        "@aws-sdk/client-s3": "^3.988.0",
        "@aws-sdk/client-textract": "^3.988.0",
        "@openrouter/ai-sdk-provider": "^2.2.3",
        "@phosphor-icons/react": "^2.1.10",
        "@tailwindcss/vite": "^4.1.18",
        "@tanstack/react-router": "^1.159.5",
        "@tanstack/router-plugin": "^1.159.5",
        "@types/bun": "^1.3.9",
        "@types/lodash": "^4.17.23",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@typescript/native-preview": "^7.0.0-dev.20260212.1",
        "@vitejs/plugin-react": "^5.1.4",
        "ai": "^6.0.83",
        "antd": "^6.3.0",
        "chalk": "^5.6.2",
        "concurrently": "^9.2.1",
        "date-fns": "^4.1.0",
        "kill-port-process": "^3.2.1",
        "lefthook": "^2.1.1",
        "lodash": "^4.17.23",
        "oxlint": "^1.47.0",
        "pdfjs-dist": "^5.4.624",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "recharts": "^3.7.0",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vite-tsconfig-paths": "^6.1.1",
        "zod": "^4.3.6"
    }
}
